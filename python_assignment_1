    return max_value 

result = find_max("Python is a programming language")
result

## or else 

def longest_word(text):
    words = text.split()
    longest = words[0] 
    for word in words:
        if len(word) > len(longest):
            longest = word
    return longest


result = longest_word("Python is a programming language")
result


