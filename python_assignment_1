# Question 10: Create a list of prime numbers between 1 and 50
print("\nQuestion 10: Create a list of prime numbers between 1 and 50")
# Your code here

def is_prime(n):
    count = 0
    if n<2 :
        print('{n}')
        return False
    for i in range(2,50):
        if n%i == 0:
            count = count + 1
    if count < 2:
        print(f"{n}")
        return True
    return False

prime_numbers = [n for n in range(1,51) if is_prime(n)]
prime_numbers


    

# Question 11: Flatten nested list [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
print("\nQuestion 11: Flatten nested list [[1, 2, 3], [4, 5, 6], [7, 8, 9]]")
# Your code here

nested = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
flattened = [num for sublist in nested for num in sublist]
flattened

# or 

nested = [[1, 5, 3], [4, 5, 6], [7, 8, 9]]
for sublist in nested:
    for num in sublist:
        flattened.append(num)
print(f"{flattened}")


# Question 12: Find common elements between [1, 2, 3, 4, 5] and [4, 5, 6, 7, 8]
print("\nQuestion 12: Find common elements between [1, 2, 3, 4, 5] and [4, 5, 6, 7, 8]")
# Your code here

nest_a = [1, 2, 3, 4, 5]
nest_b = [4, 5, 6, 7, 8]

common_elements = []

for i in nest_a:
    for j in nest_b:
        if i == j:
            common_elements.append(i)
print(f"{common_elements}")

#or 

nest_a = [1, 2, 3, 4, 5]
nest_b = [4, 5, 6, 7, 8]

nest_c = []

nest_c = set(nest_a) & set(nest_b)

print(f"{nest_c}")


